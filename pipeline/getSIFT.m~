function sifted = getSIFT(set)

    classes = 1;
    img_per_class = 1;
    
    sifted = prdataset();
    
    for i=1:classes
        for j=1:img_per_class
            img = single( getPixels(set,i,j) );
            [~,sift] = vl_dsift(img);
            sifted = [sifted; double( sift(:)' )];
        end
    end
    
    sifted.nlab = set.nlab;
    sifted.lablist = set.lablist;
    sifted.name = 'SIFT';

end